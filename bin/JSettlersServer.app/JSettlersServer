#!/bin/sh
dir="${1:-.}"   ; shift
port=${1:-8808} ; shift
n=${1:-5}       ; shift
out="${1:-/tmp/jserver.txt}"; shift
bots=${@:-dbUser dbPwd Able "''" Baker "''" Charlie "''" Dawn "''"}
echo jar dir=$dir port=$port n=$n  bots=$bots > $out

(cd $dir ;\
 echo $PWD $port $n >> $out ;\
 java -cp JSettlers.jar -jar JSettlersServer.jar $port $n ${bots} \
)
